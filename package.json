  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@jest/globals": "^28.1.3",
    "@rollup/plugin-typescript": "^4.1.2",
    "@swc/core": "^1.2.218",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^28.1.6",
    "cypress": "^5.0.0",
    "del-cli": "^3.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "prettier": "2.0.5",
    "rollup": "^2.12.0",
    "rollup-plugin-terser": "^6.1.0",
    "typescript": "^3.9.3"
  }
